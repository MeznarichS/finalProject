void lightBlueSquares() {
  fill(135, 191, 234);
  rect(70, 80, 10, 10);
  rect(190, 80, 10, 10);
  //y changes
  for (int i=100; i<=130; i+=10) {
    rect(50, i, 10, 10);
  }
  
  //y changes
  for (int i=100; i<=130; i+=10) {
    rect(210, i, 10, 10);
  }
  
  //x changes
  for (int i=70; i<=190; i+=10) {
    rect(i, 170, 10, 10);
  }
  
  //y changes
  for (int i=180; i<=230; i+=10) {
    rect(130, i, 10, 10);
  }
}

